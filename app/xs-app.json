{
  "welcomeFile": "/app/test/flpSandbox.html",
  "authenticationMethod": "route",
  "logout": {
    "logoutEndpoint": "/do/logout"
  },
  "routes": [
    {
      "source": "/srv_api/(.*)$",
      "target": "$1",
      "authenticationType": "none",
      "destination": "srv_api",
      "csrfProtection": false
    },
    {
      "source": "^/app/(.*)$",
      "target": "$1",
      "localDir": "webapp"
    }
  ]
}