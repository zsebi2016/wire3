COLUMN TABLE "EXPERT" (
	"ID" INTEGER NOT NULL,
	"REPORTS_TO" INTEGER,
	"SKILL_SET" NVARCHAR(5000),
	"EXT_ID" NVARCHAR(100),
	"NAME" NVARCHAR(100),
	"FIRSTNAME" NVARCHAR(100),
	"LASTNAME" NVARCHAR(100),
	"TITLE" NVARCHAR(100),
	"EMAIL" NVARCHAR(100),
	"DEPARTMENT" NVARCHAR(100),
	"JOBCODE" NVARCHAR(100),
	"DIVISION" NVARCHAR(100),
	"LOCATION" NVARCHAR(100),
	"CITY" NVARCHAR(100),
	"COUNTRY" NVARCHAR(100),
	"SOURCE" NVARCHAR(10),
	PRIMARY KEY ("ID")
)