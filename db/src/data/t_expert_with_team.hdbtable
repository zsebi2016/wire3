COLUMN TABLE "T_EXPERT_WITH_TEAM" ( 
     "ID" INTEGER CS_INT 
GENERATED BY DEFAULT AS IDENTITY (START WITH 200000000 
INCREMENT BY 1 MAXVALUE 2999999999) NOT NULL,
	"REPORTS_TO" INTEGER,
	"SKILL_SET" NVARCHAR(5000),
	"EXT_ID" NVARCHAR(100),
	"NAME" NVARCHAR(100),
	"TEAM_ID" INTEGER,
	"EXPERT_ID" INTEGER,
	PRIMARY KEY ("ID")
) 